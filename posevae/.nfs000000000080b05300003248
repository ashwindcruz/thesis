#!/bin/bash

# Increasing number of samples in each for loop


for i in `seq 1 1`;
do
./train.py -g 0 -o debug2_$i --model-type vae --vae-samples 1 --trans 1 --nlatent 16 --nhidden 512 \
-b 16384 --batch-limit 10 -t 180000 --time-print 600 --epoch-sample 200 --log-interval 5 --data pose\
 --init-temp 0 --temp-epoch 200 --init-learn 1e-4 --learn-decay 3e-3 --weight-decay 0 --init-model none --init-opt none
done
#	
